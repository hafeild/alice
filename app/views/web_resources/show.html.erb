<% provide(:title, "Web resource -- #{@web_resource.description}") %>
<div class="container resource-modification">
  <h1><span class="name"><%= @web_resource.description %></span>
    <% if can_edit? %>
      <%= link_to edit_web_resource_path(@web_resource)  do %>
        <button type="button" class="btn btn-default btn-md">
          <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit
        </button>
      <% end %>
    <% end %>
  </h1>


  <div class="row">
    <div class="col-md-12 col-md-offset-0">

      <%= render(partial: 'web_resource', locals: {
        print_header: false, web_resource: @web_resource}) %>
     
    </div>
  </div>
</div>
