<% provide(:title, "Assignment result -- #{@assignment_result.assignment.name}") %>
<div class="container resource-modification">
  <h1><span class="name">Assignment result for 
      "<%= @assignment_result.assignment.name %>"</span>
    <% if can_edit? %>
      <span class="edit-toggle">
        <%= link_to edit_assignment_result_path(@assignment_result)  do %>
          <button type="button" class="btn btn-default btn-md">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit
          </button>
        <% end %>
      </span>
    <% end %>
  </h1>


  <div class="row">
    <div class="col-md-12 col-md-offset-0">

      <%= render(partial: 'assignment_result', locals: {
        print_header: false, assignment_result: @assignment_result}) %>
     
    </div>
  </div>
</div>
